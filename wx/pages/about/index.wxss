/* pages/about/index.wxss */
page{
  background-color: #F3F3F3;
}
.mp-info{
  padding: 50rpx 140rpx 10rpx;
}
.mp-info-logo{
  background-color: #34619d;
  border-radius: 15rpx;
  overflow: hidden;
  box-sizing: border-box;
  padding: 16rpx;
}
.mp-info-logo>image{
  width: 100%;
  height: 100rpx;
}
.mp-info-text,.mp-info-ver{
  text-align: center;
  width: 100%;
  display: inline-block;
}
.mp-info-text{
  color: #333;
  margin: 30rpx 0 4rpx;
}
.mp-info-ver{
  color: #666;
  font-size: small;
}